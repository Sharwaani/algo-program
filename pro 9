#include<bits/stdc++.h>
using namespace std;

int main()
{
int feet,inch,yard,mile;
cout<<"Enter a measurement in feet:";
cin>>feet;
cout<<"feets to inch:\n";
inch = 12*feet;
cout<<inch<<endl;
cout<<"feets to yards:\n";
yard = 3*feet;
cout<<yard<<endl;
cout<<"feets into miles:\n";
mile = 5280*feet;
cout<<mile<<endl;
return 0;
}
	
