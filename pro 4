#include<bits/stdc++.h>
using namespace std;

int main()
{
{
	int s,s1,s2,s3,area;
	cout<<"Enter the value of s1:";
	cin>>s1;
	cout<<"Enter the value of s2:";
	cin>>s2;
	cout<<"Enter the value of s3:";
	cin>>s3;
	
	s= s1 + s2+ s3 /2;
	area = sqrt(s*(s-s1)*(s-s2)*(s-s3));
	cout<<"\nArea ="<<area;
	cout<<endl;
}
return 0;
}

